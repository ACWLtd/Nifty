{
    "name": "kjamesy/cms",
    "version": "0.3.0",
    "description": "A nifty little CMS written in Laravel 4.2 and AngularJS - intended for use in own projects",
    "authors": [
        {
            "name": "kJamesy",
            "email": "james@acw.uk.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "baum/baum": "1.0.13",
        "rydurham/sentinel": "dev-develop"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models",
            "src/controllers",
            "src/helpers"
        ],
        "psr-0": {
            "Kjamesy\\Cms\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
